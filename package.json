{
  "name": "startkit-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "check": "bun format:check && bun lint && bun typecheck && bun test",
    "clean": "git clean -xdf .next node_modules/.cache",
    "build": "bun db:migrate && next build",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "bun ./bin/db-migrate.ts",
    "dev": "next dev --turbo",
    "fix": "bun format && bun lint:fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "outdated": "bunx npm-check-updates --interactive --format group",
    "prebuild": "bun run lint",
    "start": "next start",
    "typecheck": "tsc --noEmit --pretty"
  },
  "dependencies": {
    "@lucia-auth/adapter-drizzle": "^1.1.0",
    "@neondatabase/serverless": "^0.9.5",
    "@paralleldrive/cuid2": "^2.2.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.0",
    "@t3-oss/env-nextjs": "^0.11.1",
    "arctic": "^1.9.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.33.0",
    "geist": "^1.3.1",
    "lucia": "^3.2.0",
    "lucide-react": "^0.468.0",
    "next": "^15.1.0",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^2.5.5",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.4",
    "@eslint/eslintrc": "^3",
    "@eslint/js": "^9.16.0",
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@next/mdx": "15.1.0",
    "@tailwindcss/typography": "^0.5.15",
    "@types/bun": "^1.1.14",
    "@types/mdx": "^2.0.13",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "drizzle-kit": "^0.24.2",
    "eslint": "^9",
    "eslint-config-next": "^15.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "postcss": "^8",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.16",
    "tailwindcss-animate": "^1.0.7",
    "typed-route-handler": "^1.1.0",
    "typescript": "^5",
    "typescript-eslint": "^8.18.0"
  }
}
